{
    "name": "phower/container",
    "description": "Dependency Injection for PHP complaint with Container-Interop.",
    "type": "library",
    "require": {
        "php": "^5.6 || ^7.0",
        "container-interop/container-interop": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.2",
        "squizlabs/php_codesniffer": "^2.5",
        "symfony/var-dumper": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Phower\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhowerTest\\Container\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pedro Ferreira",
            "email": "pedro@phower.com"
        }
    ]
}

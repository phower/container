{
    "name": "phower/container",
    "description": "Dependency Injection for PHP complaint with PSR-11 Container.",
    "type": "library",
    "keywords": [
        "php",
        "phower",
        "container"
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^2.8",
        "symfony/var-dumper": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Phower\\Container\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhowerTest\\Container\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Pedro Ferreira",
            "email": "pedro@phower.com"
        }
    ]
}
